        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="VerifyPortBinding">
                    <security mode="Transport" />
                </binding>
                <binding name="VerifyPortBinding1" />

        </bindings>
        <client>
            <endpoint address="https://streetmaster.streetmaster.it:443/smws/verify"
                binding="basicHttpBinding" bindingConfiguration="VerifyPortBinding"
                contract="VerifyWS.Verify" name="VerifyPort" />
        </client>
    </system.serviceModel>
